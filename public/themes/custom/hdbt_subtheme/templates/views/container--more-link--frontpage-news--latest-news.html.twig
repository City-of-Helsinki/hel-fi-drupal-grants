{% set link_attributes = {
  'class': [
    'hds-button',
    'hds-button--primary',
  ],
} %}
{% set link_title %}
  <span class="hds-button__label">{{ element['#title'] }}</span>
  {% include "@hdbt/misc/icon.twig" with {icon: 'arrow-right' } %}
{% endset %}

{{ link(link_title, element['#url'], link_attributes) }}
