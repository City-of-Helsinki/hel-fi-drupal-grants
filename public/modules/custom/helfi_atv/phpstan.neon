      count: 12
      path: tests/src/Kernel/AtvServiceTest.php

    -
      message: "#^Call to an undefined method Drupal\\\\Core\\\\Http\\\\ClientFactory\\:\\:getRequestUrl\\(\\)\\.$#"
      count: 1
      path: tests/src/Kernel/AtvServiceTest.php

    -
      message: "#^Call to an undefined method Drupal\\\\Core\\\\Http\\\\ClientFactory\\:\\:addResponse\\(\\)\\.$#"
      count: 2
