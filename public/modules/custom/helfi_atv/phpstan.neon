parameters:
  fileExtensions:
    - php
    - module
    - install
  paths:
    - ./
  excludePaths:
    - vendor
  level: 3
  checkMissingIterableValueType: false
  treatPhpDocTypesAsCertain: false
  ignoreErrors:
