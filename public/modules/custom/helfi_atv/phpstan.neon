      message: "#^Method Psr\\\\EventDispatcher\\\\EventDispatcherInterface\\:\\:dispatch\\(\\) invoked with 2 parameters, 1 required\\.$#"
      count: 2
      path: src/AtvService.php

    -
      message: "#^Property Drupal\\\\helfi_atv\\\\AtvService\\:\\:\\$maxPages \\(int\\) does not accept string\\.$#"
      count: 1
      path: src/AtvService.php

    -
      message: "#^Property Drupal\\\\helfi_atv\\\\AtvService\\:\\:\\$resultsPerPage \\(int\\) does not accept string\\.$#"
      count: 1
      path: src/AtvService.php

    -
      message: "#^Variable \\$file might not be defined\\.$#"
      count: 1
      path: src/AtvService.php

    -
      message: "#^Variable \\$file on left side of \\?\\? always exists and is not nullable\\.$#"
      count: 1
      path: src/AtvService.php

    -
      message: "#^PHPDoc tag @param for parameter \\$event with type Drupal\\\\helfi_atv\\\\Event\\\\AtvServiceEOperationEvent is not subtype of native type Drupal\\\\helfi_atv\\\\Event\\\\AtvServiceOperationEvent\\.$#"
      count: 1
      path: tests/modules/helfi_atv_test/src/EventSubscriber/AtvEventSubscriber.php

    -
