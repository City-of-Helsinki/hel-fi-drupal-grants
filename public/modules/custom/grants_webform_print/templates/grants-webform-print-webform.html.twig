<div class="webform-print-page-body">
  <div class="container">
    <section aria-label="Notification" class="hds-notification hds-notification--alert">
      <div class="hds-notification__content">
        <div class="hds-notification__label" role="heading" aria-level="2">
          <span class="hel-icon hel-icon--alert-circle-fill" aria-hidden="true"></span>
          <span>{{ 'Notice'|t }}</span>
        </div>
        <div class="hds-notification__body">
          {{ 'This printout is only for previewing the application and cannot be used when applying for a grant'|t }}
        </div>
      </div>
    </section>
  </div>
{% for key,formpage in webform %}
  {% if formpage['#type'] == 'container' %}
    <div class="webform-print-page-wizard-page container">
      <h2>{{ formpage['#title'] }}</h2>
      {% if '#' not in key %}
        {% for elementKey,element in formpage %}
          {% set is_not_hidden = element['#type'] != 'hidden' %}
          {% if is_not_hidden %}
            {% if isArray(element) %}
              {{ element }}
{#              {{ breakpoint() }}#}
            {% endif %}
          {% endif %}
        {% endfor %}
      {% endif %}

    </div>
  {% endif %}
{% endfor %}
</div>
