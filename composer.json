{
    "name": "city-of-helsinki/drupal-helfi-platform",
    "description": "Helsinki City grants.",
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/City-of-Helsinki/drupal-helfi-platform",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "2024.16",
    "require": {
        "ext-json": "*",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.6.7",
        "doctrine/lexer": "^2",
        "drupal/autologout": "^2",
        "drupal/better_exposed_filters": "^7",
        "drupal/block_field": "^1.0@RC",
        "drupal/content_access": "^2.0",
        "drupal/core-composer-scaffold": "^10|^11",
        "drupal/disable_messages": "^2.1",
        "drupal/draggableviews": "^2.0",
        "drupal/entity_type_clone": "^4.0",
        "drupal/entityreference_filter": "^2.0",
        "drupal/error_page": "^2.0",
        "drupal/externalauth": "^2.0",
        "drupal/hdbt": "^6.0",
        "drupal/hdbt_admin": "^3.0",
        "drupal/helfi_azure_fs": "^2",
        "drupal/helfi_drupal_tools": "dev-main",
        "drupal/helfi_platform_config": "^4.4",
        "drupal/helfi_proxy": "^3.0",
        "drupal/helfi_tpr": "^2.1",
        "drupal/helfi_tunnistamo": "^3.0",
        "drupal/jquery_ui_draggable": "^2.0",
        "drupal/js_cookie": "^1.0",
        "drupal/jsonapi_extras": "^3.23",
        "drupal/listjs": "^2.0",
        "drupal/menu_admin_per_menu": "^1.5",
        "drupal/multivalue_form_element": "^1.0@beta",
        "drupal/override_node_options": "^2.7",
        "drupal/purge_users": "^3.1",
        "drupal/radioactivity": "^4.0",
        "drupal/redis": "^1.7",
        "drupal/remove_http_headers": "^2.0.1",
        "drupal/restui": "^1.22",
        "drupal/session_limit": "^2.0@beta",
        "drupal/translated_config": "^1.1",
        "drupal/tvp": "^1.0",
        "drupal/webform": "^6.3",
        "drupal/webform_rest": "^4.0",
        "drupal/webform_translation_permissions": "^2.1",
        "drupal/webform_views": "^5.2",
        "globalcitizen/php-iban": "^4.1",
        "guzzlehttp/guzzle": "^7",
        "justinrainbow/json-schema": "^6",
        "league/openapi-psr7-validator": "^0.22.0",
        "mglaman/composer-drupal-lenient": "*",
        "nesbot/carbon": "^2.64",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "ramsey/uuid": "^4.3",
        "symfony/console": "^6.4",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "dg/bypass-finals": "^1.5",
        "donatj/mock-webserver": "^2.4",
        "drupal/core-dev": "^10|^11",
        "jangregor/phpstan-prophecy": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "weitzman/drupal-test-traits": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "mglaman/composer-drupal-lenient": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": false,
            "wikimedia/composer-merge-plugin": true
        },
        "audit": {
            "abandoned": "report",
            "ignore": ["GHSA-mg8j-w93w-xjgc"]
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/core": {
                "Fix missing wrapper from core form": "patches/fix-form-wrapper.patch",
                "#3023228: Status messages show up twice": "https://www.drupal.org/files/issues/2023-07-19/3023228-39.patch",
                "Asset resolver empty settings cache issue": "patches/assetresolver-settings.patch",
                "Pseudonymize logging of user data.": "patches/drupal_10.5.3-user-module-gdpr-pseudonymize-logging.patch",
                "Change file name length constraint to 100 characters": "patches/drupal-10.5.x-file-name-length-constraint-100-chars.patch",
                "Add ability to delete all from tempstore": "https://www.drupal.org/files/issues/2020-10-23/get_delete_all_temp-2475719-31.patch"
            },
            "drupal/autologout": {
                "Modal related issues": "https://www.drupal.org/files/issues/2023-04-25/autologout.2023-04-25.patch",
                "Pseudonymize logging of user data.": "patches/autologout_2.0.0-gdpr-pseudonymize-logging.patch"
            },
            "drupal/crop": {
                "Fix encoded paths for files": "patches/AU-2225-fix-encoded-paths.patch"
            },
            "drupal/externalauth": {
                "Pseudonymize logging of user data.": "patches/externalauth-2.0.8-gdpr-pseudonymize-logging.patch"
            },
            "drupal/helfi_helsinki_profiili": {
                "Pseudonymize logging of user data.": "patches/helfi_helsinki_profiili-0.9.30-gdpr-pseudonymize-logging.patch"
            }
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "./public"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "public/core": [
                "type:drupal-core"
            ],
            "public/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "public/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "public/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "public/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "public/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "public/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/big_pipe_sessionless"]
        },
        "merge-plugin": {
            "include": [
                "public/modules/contrib/webform/composer.libraries.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-replace": true,
            "merge-scripts": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repository.drupal.hel.ninja"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "copy-commit-message-script": "make copy-commit-message-script",
        "post-install-cmd": [
            "@copy-commit-message-script"
        ],
        "post-update-cmd": [
            "@copy-commit-message-script"
        ],
        "test": [
            "@lint-php",
            "@test-php"
        ],
        "test-php": "vendor/bin/phpunit -c $PWD/phpunit.xml.dist",
        "lint-php": "vendor/bin/phpcs --standard=Drupal,DrupalPractice --extensions=php,inc,test,info,module,install"
    },
    "autoload": {
        "psr-4": {
            "Drush\\Style\\": "vendor/drush/drush/src-symfony-compatibility/v6/Style",
            "Drush\\Symfony\\": "vendor/drush/drush/src-symfony-compatibility/v6/Symfony"
        }
    }
}
